@prefix : <http://w3id.org/deer/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

:node_reader1
              a                  :FileModelReader ;
              :implementedIn     :FileModelReader ;
              :fromUri           <http://dbpedia.org/resource/Paderborn> ;
              :hasOutput         ( :node_geofusion ) .

:node_reader2
              a                  :FileModelReader ;
              :implementedIn     :FileModelReader ;
              :fromUri           <http://linkedgeodata.org/triplify/node240114473> ;
              :hasOutput         ( :node_conf ) .

:node_conf
              a                  :AuthorityConformationEnrichmentOperator ;
              :implementedIn     :AuthorityConformationEnrichmentOperator ;
              :hasInput          ( :node_reader2 ) ;
              :hasOutput         ( :node_geofusion ) ;
              :sourceSubjectAuthority
                                 "http://dbpedia.org" ;
              :targetSubjectAuthority
                                 "http://deer.org" .

:node_geofusion
              a                  :GeoFusionEnrichmentOperator ;
              :implementedIn     :GeoFusionEnrichmentOperator ;
              :hasInput          ( :node_conf :node_reader1 ) ;
              :hasOutput         ( :node_filter ) ;
              :fusionAction      "takeAll" ;
              :mergeOtherStatements
                                 "true" .

:node_filter
              a                  :FilterEnrichmentOperator ;
              :implementedIn     :FilterEnrichmentOperator ;
              :hasInput          ( :node_geofusion ) ;
              :hasOutput         ( :node_writer ) ;
              :selectors         (
                    [ :predicate geo:lat ]
                    [ :predicate geo:long ]
                    [ :predicate rdfs:label ]
                    [ :predicate owl:sameAs ]
              ) .

:node_writer
              a                  :DefaultModelWriter ;
              :implementedIn     :DefaultModelWriter ;
              :outputFile        "output_demo.ttl" ;
              :outputFormat      "Turtle" ;
              :hasInput          ( :node_filter ) .

:FileModelReader
  rdfs:subClassOf :ExNode .
:GeoFusionEnrichmentOperator
  rdfs:subClassOf :ExNode .
:FilterEnrichmentOperator
  rdfs:subClassOf :ExNode .
:DefaultModelWriter
  rdfs:subClassOf :ExNode .
:AuthorityConformationEnrichmentOperator
  rdfs:subClassOf :ExNode .